{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": ["Android >= 4.1", "iOS >= 7"]
        }
      }
    ],
    "stage-2"
  ],
  "comments": false,
  "plugins": [
    "transform-vue-jsx",
    "transform-runtime",
    [
      "import",
      {
        "libraryName": "vant",
        "libraryDirectory": "es",
        "style": true
      }
    ]
  ]
}
